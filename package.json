		"autoprefixer": "^9.0.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.0.6",
		"babel-preset-env": "^1.7.0",
		"bootstrap": "^4.3.1",
		"chai": "^3.5.0",
		"chai-subset": "^1.6.0",
		"chalk": "^1.1.3",
		"cheerio": "0.22.0",
		"cldr-data": "^32.0.1",
