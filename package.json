{
	"name": "ecommpay_sfcc",
	"version": "1.0.0",
	"description": "Ecommpay Salesforce Commerce Cloud Payment Plugin",
	"main": "index.js",
	"engines": {
		"node": ">=16.0.0 <17.0.0"
	},
	"scripts": {
		"compile:scss": "sgmf-scripts --compile css",
		"compile:js": "sgmf-scripts --compile js",
		"build": "npm run compile:js && npm run compile:scss",
		"upload": "sgmf-scripts --upload -- ",
		"uploadCartridge": "sgmf-scripts --uploadCartridge *ecommpay* && npm run uploadSfraBase",
		"uploadSfraBase": "sgmf-scripts --uploadCartridge *_storefront_base && sgmf-scripts --uploadCartridge modules",
		"watch": "concurrently \"npm:watch:*\"",
		"watch:uploader": "sgmf-scripts --watch",
		"watch:static": "sgmf-scripts --watch static",
		"zipMetadata": "zip -rq ecp_meta.zip ecp_meta"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ITECOMMPAY/ecommpay-sfcc"
	},
	"author": "Ecommpay <CHANGE_ME@gmail.com>",
	"license": "ISC",
	"homepage": "https://github.com/ITECOMMPAY",
